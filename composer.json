{
  "name": "eugenerg/event_hub-microsoft-calendar-adapter",
  "description": "Google Calendar API Client Library",
  "type": "library",
  "autoload": {
    "psr-4": {
      "EugeneErg\\EventHubMicrosoftCalendarAdapter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "microsoft/microsoft-graph": "^2.1"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
